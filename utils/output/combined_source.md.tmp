    margin-bottom: 0.5rem;
  }

  .post-excerpt {
    margin-left: 10px;
    margin-bottom: 0.2rem;
  }

  .p {
    font-size: 1.1rem;
    line-height: 1.5rem;
    margin-bottom: 1.2rem;
  }

